{
    "name": "sqlite3-promise-query-api",
    "version": "0.0.10",
    "description": "A promise wrapper around sqlite3 with additional query and type support",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "directories": {
        "lib": "./lib"
    },
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.10",
        "@types/node-fetch": "^2.6.2",
        "@types/sqlite3": "^3.1.8",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "chai": "^4.3.7",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "mocha": "^10.1.0",
        "moment": "^2.29.4",
        "nyc": "^15.1.0",
        "prettier": "^2.8.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3",
        "winston": "^3.8.2"
    },
    "scripts": {
        "prepare": "tsc",
        "build": "tsc",
        "build:check": "tsc --noEmit",
        "lint": "eslint src test",
        "lint:fix": "eslint --fix src test",
        "format": "prettier --check src test .prettierrc.js .eslintrc.js",
        "format:fix": "prettier --write src test .prettierrc.js .eslintrc.js",
        "test": "mocha --require ts-node/register test/**/**/*.test.ts test/**/*.test.ts test/*.test.ts",
        "nyc": "nyc --reporter=html --reporter=text mocha --require ts-node/register test/**/**/*.test.ts test/**/*.test.ts test/*.test.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AnonymerNiklasistanonym/sqlite3-promise-query-api.git"
    },
    "keywords": [
        "api",
        "osu",
        "async",
        "promise",
        "typescript"
    ],
    "author": {
        "name": "AnonymerNiklasistanonym"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/AnonymerNiklasistanonym/sqlite3-promise-query-api/issues"
    },
    "homepage": "https://github.com/AnonymerNiklasistanonym/sqlite3-promise-query-api#readme",
    "dependencies": {
        "sqlite3": "^5.1.2"
    }
}
